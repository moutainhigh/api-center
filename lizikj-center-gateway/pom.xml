    <build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<includes>
					<include>**/*.properties</include>
					<include>**/*.Filter</include>
					<include>**/*.xml</include>
					<include>**/*.vm</include>
					<include>**/*.xlsx</include>
					<include>**/*.ftl</include>
					<include>**/*.xls</include>
				</includes>
				<excludes>
					<exclude>mybatis-generator/**</exclude>
				</excludes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<executions>
					<execution>
						<goals>
							<goal>repackage</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<!-- 指定maven 编译的JDK版本和编码格式 -->
